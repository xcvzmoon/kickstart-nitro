{
  "name": "@xcvzmoon/kickstart-nitro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "bunx --bun nitro build",
    "dev": "bunx --bun nitro dev",
    "start": "bun .output/server/index.mjs",
    "format": "oxfmt && oxfmt --check",
    "lint": "oxlint --type-aware",
    "db:generate": "bunx --bun drizzle-kit generate",
    "db:push": "bunx --bun drizzle-kit push",
    "db:pull": "bunx --bun drizzle-kit pull",
    "db:studio": "bunx --bun drizzle-kit studio",
    "db:check": "bunx --bun drizzle-kit check"
  },
  "dependencies": {
    "drizzle-orm": "^1.0.0-beta.11-05230d9",
    "drizzle-query-logger": "^1.1.2",
    "drizzle-zod": "^0.8.3",
    "h3-compression": "^0.3.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/bun": "^1.3.6",
    "drizzle-kit": "^1.0.0-beta.11-05230d9",
    "h3": "^1.15.5",
    "nitropack": "^2.13.1",
    "oxfmt": "^0.26.0",
    "oxlint": "^1.41.0",
    "oxlint-tsgolint": "^0.11.1",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.3"
  },
  "packageManager": "bun@1.3.6"
}
